	"\0                         ",  // the control name .... Alternative
    RDOUT_STRING_TABLE,
    IDB_ZOOM_SMALLKNOB, 0x0000, 0x0000,

    // Input Mute Button ########################		
    //------------------
    2, 405,   12, 415,
    CTRL_NUM_INPUT_MUTE, CTRL_TYPE_BTN_INPUTMUTE_FILTER, DISP_TYPE_NORMAL,
	"MUTE THE GATE\0            ",  // the control name ....
	"\0                         ",  // the control name .... Alternative
    RDOUT_STRING_TABLE,
    0x0000, IDB_FULL_I_MUTE_DOWN, 0x0000,

