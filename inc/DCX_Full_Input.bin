    1, 214,     13, 401,
    CTRL_NUM_NULL, CTRL_TYPE_OPEN_FADER, DISP_TYPE_NULL,
	"\0                         ",  // no control name ....
	"\0                         ",  // the control name .... Alternative
    RDOUT_NORDOUT,
    0x0000, 0x0000, 0x0000,

    // The Label Display
    1, 213,     13, 402,
    250, CTRL_TYPE_CHANNEL_NAME_DISPLAY_VERTICAL, DISP_TYPE_NORMAL,
	"\0                         ",  // no control name ....
	"\0                         ",  // the control name .... Alternative
    RDOUT_NORDOUT,
    0x0000, 0x0000, 0x0000,

