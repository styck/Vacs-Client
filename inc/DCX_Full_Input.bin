	"\0                         ",  // the control name .... Alternative
    RDOUT_STRING_TABLE,
    IDB_FDR_WHITE_TINY, 0x0000, 0x0000,






	// terminator ....
    0x0000, 0x0000, 0x0000, 0x0000,
    CTRL_NUM_NULL, CTRL_TYPE_NULL, DISP_TYPE_NULL,
	"\0                         ",  // no control name ....
	"\0                         ",  // the control name .... Alternative
    RDOUT_NORDOUT,
    0x0000, 0x0000, 0x0000,
END
