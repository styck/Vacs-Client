    1, 129,     13, 174,
    CTRL_NUM_NULL, CTRL_TYPE_OPEN_ZOOM_SUB, DISP_TYPE_NULL,
	"\0                         ",  // no control name ....
	"\0                         ",  // the control name .... Alternative
    RDOUT_NORDOUT,
    0x0000, 0x0000, 0x0000,

